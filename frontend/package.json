{
  "name": "wanderflow-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "WanderFlow - AI Travel Planner Frontend",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "pinia": "^2.1.7",
    "axios": "^1.6.7",
    "@vueuse/core": "^10.9.0",
    "date-fns": "^3.3.1",
    "nprogress": "^0.2.0",
    "highlight.js": "^11.9.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "vite": "^5.1.5",
    "vue-tsc": "^2.0.typescript": "^56",
    ".4.2",
    "tailwindcss": "^3.4.1",
    "postcss": "^8.4.35",
    "autoprefixer": "^10.4.18",
    "@types/node": "^20.11.25",
    "@types/nprogress": "^0.2.3",
    "eslint": "^8.57.0",
    "@typescript-eslint/parser": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "eslint-plugin-vue": "^9.22.0",
    "prettier": "^3.2.5",
    "husky": "^9.0.11",
    "@commitlint/cli": "^19.0.3",
    "@commitlint/config-conventional": "^19.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
